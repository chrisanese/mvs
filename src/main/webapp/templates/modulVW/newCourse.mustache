	});
	
	$('#saveInhalt').click(function(){
		var newInhalt = nicEditors.findEditor('lvInhalt').getContent();
		if(newInhalt != oldInhalt){
			$('#lvInhalt').attr('data-edit','true');
			oldInhalt = newInhalt;
		}
